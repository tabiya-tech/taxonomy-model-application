{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@testing-library/jest-dom": "~5.16.4",
    "@testing-library/react": "~13.4.0",
    "@testing-library/user-event": "~14.4.3",
    "@types/jest": "~29.0.1",
    "@types/node": "~16.7.13",
    "@types/react": "~18.0.0",
    "@types/react-dom": "~18.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@types/supertest": "~2.0.12",
    "http-status-codes": "~2.2.0",
    "react-scripts": "~5.0.1",
    "rimraf": "~3.0.2",
    "run-script-os": "~1.1.6",
    "supertest": "~6.3.3",
    "jest": "~29.5.0",
    "ts-jest": "~29.1.0",
    "typescript": "~4.4.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/material": "^5.12.3",
    "ajv": "~8.12.0",
    "ajv-formats": "~2.1.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-router-dom": "^6.10.0",
    "web-vitals": "~2.1.0"
  },
  "scripts": {
    "clean": "rimraf -I build coverage",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "test:smoke": "jest --config jest.smoke.config.js --runInBand test/smoke/*.test.ts",
    "postinstall:win32": "cd .. & link-api-specifications.bat",
    "postinstall:darwin:linux": "(cd .. && ./link-api-specifications.sh)",
    "postinstall": "run-script-os"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      [
        "lcov",
        {
          "projectRoot": "../"
        }
      ],
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
