{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@faker-js/faker": "~8.0.1",
    "@storybook/addon-a11y": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.4.0",
    "@storybook/preset-create-react-app": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-webpack5": "^7.4.0",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "~0.2.0",
    "@testing-library/jest-dom": "~5.16.4",
    "@testing-library/react": "~13.4.0",
    "@testing-library/user-event": "~14.4.3",
    "@types/jest": "~29.0.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "~16.7.13",
    "@types/papaparse": "^5.3.14",
    "@types/react": "~18.0.0",
    "@types/react-dom": "~18.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@types/supertest": "~2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "axe-playwright": "^1.2.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "~8.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "http-server": "~14.1.1",
    "http-status-codes": "~2.2.0",
    "jest": "~29.5.0",
    "jest-extended": "~4.0.2",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "rimraf": "~3.0.2",
    "run-script-os": "~1.1.6",
    "storybook": "^7.4.0",
    "storybook-addon-mock": "^4.2.1",
    "supertest": "~6.3.3",
    "ts-jest": "~29.1.0",
    "typescript": "5.2.2",
    "uuid": "~9.0.0",
    "wait-for": "~2.1.1",
    "webpack": "^5.83.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@material-ui/styles": "^4.11.5",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.167",
    "@mui/material": "^5.15.12",
    "ajv": "~8.12.0",
    "ajv-formats": "~2.1.1",
    "create-react-app": "^5.0.1",
    "framer-motion": "^11.0.5",
    "lodash.debounce": "^4.0.8",
    "notistack": "^3.0.1",
    "p-limit": "^4.0.0",
    "papaparse": "^5.4.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "~2.1.0"
  },
  "prettier": "@tabiya/prettier-config",
  "scripts": {
    "lint": "eslint src --ext .ts --ext .tsx --max-warnings 0",
    "clean": "rimraf -I build coverage storybook-static",
    "compile": "tsc --noEmit",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "test:smoke": "jest --config jest.smoke.config.js --runInBand test/smoke/*.test.ts",
    "postinstall:win32": "cd .. & link-api-specifications.bat & link-tabiya-prettier-config.bat",
    "postinstall:darwin:linux": "(cd .. && ./link-api-specifications.sh && ./link-tabiya-prettier-config.sh)",
    "postinstall": "run-script-os",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook",
    "test:accessibility": "npx concurrently -k -s first -n 'STORYBOOK,TEST' -c 'magenta,blue' 'npx http-server storybook-static --port 6007 --silent' 'npx wait-on http://127.0.0.1:6007 && yarn test-storybook --url http://127.0.0.1:6007'",
    "build-storybook": "storybook build",
    "format:check": "prettier --check src/",
    "format": "prettier --write src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "import/first": "off"
    }
  },
  "jest": {
    "resetMocks": false,
    "clearMocks": false,
    "transformIgnorePatterns": [
      "node_modules/(?!p-limit)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/_test_utilities/*",
      "!**/**/*.d.ts",
      "!**/**/*.types.ts"
    ],
    "coverageReporters": [
      [
        "lcov",
        {
          "projectRoot": "../"
        }
      ],
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
