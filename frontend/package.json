{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.12",
    "@storybook/preset-create-react-app": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/react-webpack5": "^7.0.12",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "~5.16.4",
    "@testing-library/react": "~13.4.0",
    "@testing-library/user-event": "~14.4.3",
    "@types/jest": "~29.0.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "~16.7.13",
    "@types/react": "~18.0.0",
    "@types/react-dom": "~18.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@types/supertest": "~2.0.12",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-plugin-storybook": "^0.6.12",
    "http-status-codes": "~2.2.0",
    "jest": "~29.5.0",
    "prop-types": "^15.8.1",
    "rimraf": "~3.0.2",
    "run-script-os": "~1.1.6",
    "storybook": "^7.0.12",
    "supertest": "~6.3.3",
    "ts-jest": "~29.1.0",
    "typescript": "~4.4.2",
    "webpack": "^5.83.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@material-ui/styles": "^4.11.5",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.1",
    "@storybook/addon-a11y": "^7.0.12",
    "ajv": "~8.12.0",
    "ajv-formats": "~2.1.1",
    "create-react-app": "^5.0.1",
    "lodash.debounce": "^4.0.8",
    "p-limit": "^4.0.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "~2.1.0"
  },
  "scripts": {
    "lint": "eslint src --ext .ts --ext .tsx",
    "clean": "rimraf -I build coverage",
    "compile": "tsc --noEmit",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "test:smoke": "jest --config jest.smoke.config.js --runInBand test/smoke/*.test.ts",
    "postinstall:win32": "cd .. & link-api-specifications.bat",
    "postinstall:darwin:linux": "(cd .. && ./link-api-specifications.sh)",
    "postinstall": "run-script-os",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "rules": {
      "import/first": "off"
    }
  },
  "jest": {
    "resetMocks": false,
    "clearMocks": false,
    "transformIgnorePatterns": [
      "node_modules/(?!p-limit)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/*.stories.ts"
    ],
    "coverageReporters": [
      [
        "lcov",
        {
          "projectRoot": "../"
        }
      ],
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
