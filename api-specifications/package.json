{
  "name": "api-specifications",
  "version": "1.0.0",
  "license": "MIT License",
  "dependencies": {
    "ajv": "~8.12.0"
  },
  "prettier": "@tabiya/prettier-config",
  "scripts": {
    "lint": "eslint src --ext .ts --ext .tsx --max-warnings 0",
    "precompile": "rimraf -I dist coverage schemas",
    "compile": "tsc --build tsconfig.pub.json",
    "test": "jest --coverage",
    "postcompile": "cp package.json dist/package.json",
    "postinstall:win32": "cd .. & link-tabiya-prettier-config.bat",
    "postinstall:darwin:linux": "(cd .. && ./link-tabiya-prettier-config.sh)",
    "postinstall": "run-script-os",
    "pregenerateSchemas": "rimraf -I schemas",
    "generateSchemas": "ts-node generateJsonSchemaFiles.ts './src/error/index.ts' './src/importProcessState/index.ts' './src/info/index.ts' './src/locale/index.ts' './src/modelInfo/index.ts' './src/presigned/index.ts'",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/jest": "~29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "ajv": "~8.12.0",
    "ajv-formats": "~2.1.1",
    "archiver": "^5.3.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "~29.1.0",
    "jest-performance-matchers": "~1.0.0",
    "prettier": "^3.0.3",
    "rimraf": "~3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "~29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "main": "dist/"
}
