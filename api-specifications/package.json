    "generateSchemas": "ts-node generateJsonSchemaFiles.ts './src/error/index.ts' './src/importProcessState/index.ts' './src/info/index.ts' './src/locale/index.ts' './src/modelInfo/index.ts' './src/presigned/index.ts'",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/jest": "~29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "ajv": "~8.12.0",
    "ajv-formats": "~2.1.1",
    "archiver": "^5.3.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "~29.1.0",
    "jest-performance-matchers": "~1.0.0",
    "prettier": "^3.0.3",
    "rimraf": "~3.0.2",
    "ts-jest": "~29.1.0",
    "ts-node": "~10.4.0",
    "typescript": "~4.4.4"
  },
  "main": "dist/"
}
