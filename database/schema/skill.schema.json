{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "ModelId": {
        "type": "integer"
      },
      "SkillID": {
        "type": "integer"
      },
      "UUID": {
        "type": "string"
      },
      "OriginUUID": {
        "type": "string"
      },
      "ESCOUri": {
        "type": "string"
      },
      "Tabiya": {
        "type": "string"
      },
      "SkillType": {
        "type": "string"
      },
      "reuseLevel": {
        "type": "string"
      },
      "ScopeNote": {
        "type": "string"
      },
      "PreferredLabel": {
        "type": "string"
      },
      "AltLabels": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "Definition": {
        "type": "string"
      },
      "Description": {
        "type": "string"
      },
      "Skills": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "SkillID": {
              "type": "integer"
            },
            "SkillUUD": {
              "type": "string",
              "enum": [
                "gh"
              ]
            },
            "RelationType": {
              "type": "string",
              "enum": [
                "optional"
              ]
            }
          },
          "required": [
            "SkillID",
            "SkillUUD",
            "RelationType"
          ]
        }
      },
      "Parents": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "ParentID": {
              "type": "integer"
            },
            "ParentUUID": {
              "type": "string",
              "enum": [
                "O"
              ]
            },
            "ParentObjectType": {
              "type": "string"
            }
          },
          "required": [
            "ParentID",
            "ParentUUID",
            "ParentObjectType"
          ]
        }
      }
    },
    "required": [
      "ModelId",
      "SkillID",
      "UUID",
      "OriginUUID",
      "ESCOUri",
      "Tabiya",
      "SkillType",
      "reuseLevel",
      "ScopeNote",
      "PreferredLabel",
      "AltLabels",
      "Definition",
      "Description",
      "Skills",
      "Parents"
    ]
  }